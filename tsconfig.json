{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6",
         "dom",
         "es2017"
      ],
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true
   },
   "include": [
       "src/**/*"
   ],
   "exclude": [
      "node_modules",
      "**/*.spec.ts",
      "swaggerFiles"
  ]
}